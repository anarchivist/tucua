    <skos:narrower rdf:resource="http://www2.archivists.org/glossary/terms/c/content"/>
    <skos:narrower rdf:resource="http://www2.archivists.org/glossary/terms/s/structure"/>
    <skos:narrower rdf:resource="http://www2.archivists.org/glossary/terms/c/contingent-records"/>
    <skos:narrower rdf:resource="http://www2.archivists.org/glossary/terms/m/memorial-record"/>
